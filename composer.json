{
    "name": "smith981/crud-generator",
    "description": "This bundle generates a nice CRUD",
    "keywords": ["crud", "generator"],
    "homepage": "https://github.com/smith981/CrudGeneratorBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Ed Smith",
            "email": "smith981@gmail.com"
        }
    ],
    "require": {
        "symfony/symfony": "2.2.*",
        "sensio/generator-bundle": "2.2.*",
        "pagerfanta/pagerfanta": "1.0.*@dev",
        "lexik/form-filter-bundle": "1.2.*"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-0": { "Smith981\\CrudGeneratorBundle": "" },
        "files": [ "Smith981CrudGeneratorBundle.php", "Smith981\\CrudGeneratorBundle\\Command\\Smith981CrudCommand" ]
    },
    "target-dir": "Smith981/Bundle/CrudGeneratorBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "2.2.x-dev"
        }
    }
}
